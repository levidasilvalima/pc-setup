{
    "vim.visualModeKeyBindings": [
    {
        "before": ["<C-h>"],
        "after": ["<C-w>", "h"]
    },
    {
        "before": ["<C-j>"],
        "after": ["<C-w>", "j"]
    },
    {
        "before": ["<C-k>"],
        "after": ["<C-w>", "k"]
    },
    {
        "before": ["<C-l>"],
        "after": ["<C-w>", "l"]
    },
    {
        "before": ["<S-h>"],
        "commands": [":tabprevious"]
    },
    {
        "before": ["<S-l>"],
        "commands": [":tabnext"]
    }
    ],
    "vim.insertModeKeyBindings": [
    {
        "before": ["k", "k"],
        "after": ["<Esc>"]
    }
    ],
    "vim.normalModeKeyBindings": [
    {
        "before": ["<C-h>"],
        "after": ["<C-w>", "h"]
    },
    {
        "before": ["<C-j>"],
        "after": ["<C-w>", "j"]
    },
    {
        "before": ["<C-k>"],
        "after": ["<C-w>", "k"]
    },
    {
        "before": ["<C-l>"],
        "after": ["<C-w>", "l"]
    },
    {
        "before": ["<S-h>"],
        "commands": [":tabprevious"]
    },
    {
        "before": ["<S-l>"],
        "commands": [":tabnext"]
    }
    ]
}
